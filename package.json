{
    "name": "codearts-example-project-wizard",
    "publisher": "",
    "author": "",
    "keywords": [
        "cloudide",
        "plugin"
    ],
    "version": "0.0.1",
    "license": "SEE LICENSE IN LICENSE",
    "description": "",
    "displayName": "codearts-example-project-wizard",
    "icon": "resources/icons/logo.png",
    "files": [
        "src"
    ],
    "main": "dist/plugin.js",
    "activationEvents": [
        "*"
    ],
    "devDependencies": {
        "@codearts/plugin": "latest",
        "@codearts/plugin-packager": "latest",
        "rimraf": "2.6.2",
        "ts-loader": "^8.3.0",
        "typescript": "^4.4.0",
        "typescript-formatter": "7.2.2",
        "webpack": "^4.39.2",
        "webpack-cli": "^3.3.7"
    },
    "dependencies": {
        "@cloudide/messaging": "latest",
        "@cloudide/nls": "latest",
        "@codearts/core": "latest"
    },
    "scripts": {
        "prepare": "npm run clean && npm run build",
        "clean": "rimraf dist",
        "format-code": "tsfmt -r",
        "watch": "tsc -watch",
        "compile": "tsc",
        "package": "codearts-pack -p",
        "build": "npm run format-code && npm run compile && npm run build-browser",
        "build-browser": "webpack --config webpack.config.js",
        "watch-browser": "webpack --config webpack.config.js --watch"
    },
    "engines": {
        "codearts": "^2.0.0"
    }
}
